<tool id="ctxcal" name="ctxcal">
   <description>
Convert camera image to a map projection
  </description>

   <command>ctxcal from=$from to=$to flatfile=$flatfile</command>

    <inputs>
   	 <param name="from" format="cub" type="data" label="FROM="/>
   	 <param name="flatfile" format="cub" type="data" label="FLATFILE="/>

   	 <param name="IOF" type="boolean" checked="true"/>
    </inputs>

    <outputs>
   	 <data name="TO" format="cub" label="TO="/>
    </outputs>

<help>
Takes and outputs .cub file
</help>
</tool>
