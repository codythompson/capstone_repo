<tool id="ctxcal_isisToolExec" name="ctxcal via isisToolExecutor">
  	<description>
		Executes a specified command line tool.
 	</description>

  	<command interpreter="python">isisToolExecutor.py ctxcal from=$from to=$to</command>

  	<inputs>
    		<param name="from" format="cub" type="data" label="Input cub"/>
  	</inputs>

	<outputs>
		<data name="to" format="cub" label="ctxcal Output cub"/>
	</outputs>

	<help>
		Generic Help Message... replace me.
	</help>
</tool>
