<tool id="mroctx2isis" name="mroctx2isis">
   <description>
Convert camera image to a map projection
  </description>

   <command>mroctx2isis from=$from to=$to</command>

    <inputs>
   	 <param name="FROM" format="img" type="data" label="FROM="/>
	 
	 <param name="PREFIX" type="integer" value="Computed"/>
	 <param name="SUFFIX" type="integer" value="Computed"/>
	 <param name="FILLGAP" type="boolean" checked="true"/>
    </inputs>

    <outputs>
   	 <data name="TO" format="cub" label="TO="/>
    </outputs>

<help>
Input must be an IMG file
</help>
</tool>
