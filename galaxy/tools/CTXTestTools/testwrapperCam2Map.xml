<tool id="cam2map_isisToolExec" name="cam2map">
  	<description>
		Convert camera image to a map projection
 	</description>

  	<command interpreter="python">isisToolExecutor.py cam2map from=$from map=$map to=$to</command>

  	<inputs>
    		<param name="from" format="img" type="data" label="from="/>
		<param name="map" format="map" type="data" label="map="/>
  	</inputs>

	<outputs>
		<data name="to" format="cub" label="to="/>
	</outputs>

	<help>
		Remember to specify a .map file
	</help>
</tool>
