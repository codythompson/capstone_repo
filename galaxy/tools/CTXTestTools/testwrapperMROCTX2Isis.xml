<tool id="mroctx2isis_isisToolExec" name="mroctx2isis via isisToolExecutor">
  	<description>
		Executes a specified command line tool.
 	</description>

  	<command interpreter="python">isisToolExecutor.py ctxcal from=$from to=$to</command>

  	<inputs>
    		<param name="from" format="img" type="data" label="Input IMG"/>
  	</inputs>

	<outputs>
		<data name="to" format="cub" label="mroctx2isis Output cub"/>
	</outputs>

	<help>
		Generic Help Message... replace me.
	</help>
</tool>
